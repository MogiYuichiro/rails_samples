<p id="notice"><%= notice %></p>

<h1>Listing Omakes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th>Weight</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @omakes.each do |omake| %>
      <tr>
        <td><%= omake.name %></td>
        <td><%= omake.price %></td>
        <td><%= omake.weight %></td>
        <td><%= link_to 'Show', omake %></td>
        <td><%= link_to 'Edit', edit_omake_path(omake) %></td>
        <td><%= link_to 'Destroy', omake, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Omake', new_omake_path %>
